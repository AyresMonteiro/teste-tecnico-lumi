{
    "version": 2,
    "builds": [
        {
            "src": "backend/src/function.ts",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/src/package.json",
            "use": "@vercel/next"
        }
    ],
    "rewrites": [
        {
            "source": "/web/(.*)",
            "destination": "/frontend/src/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/backend/src/function.ts"
        }
    ]
}
