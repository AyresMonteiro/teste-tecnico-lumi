{
    "builds": [
        {
            "src": "backend/dist/src/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["backend/dist/**"] }
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/$1"
        }
    ]
}
