{
    "builds": [
        {
            "src": "backend/package.json",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/$1"
        },
        {
            "source": "/web/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}
